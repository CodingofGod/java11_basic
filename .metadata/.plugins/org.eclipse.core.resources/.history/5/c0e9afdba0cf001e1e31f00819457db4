package org.kh.java;

public class Variable4 {
	public static void main(String[] args) {
		String name = "\'강민우\'";
		short age = 26;
		float height = 173.6f;
		double weight = 69.3f;
		char init = 'k';		
		
		System.out.println("이름 : "+name);
		/* 이스케이프 문자
		 \n : 줄바꿈
		 \t : 탭
		 ★\\ : \를 출력★
		 
		 */
	}

}
