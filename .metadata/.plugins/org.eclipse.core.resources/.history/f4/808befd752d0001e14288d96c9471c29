package org.kh.site2;
//대입 연산자 : 연산 후에 대입하는 연산자 +=, -=, *=, /=, &=, |=, >>=, <<=, >>>=, ^=
public class Operation7 {
	public static void main(String[] args) {
		int a = 20;
		int b = 15;
		int tmp; // tmp : 임시 기억 장소
		a += b; 	//a=35, b=15
		b -= a;		//a=35, b=-20
		//a와 b 의 값을 맞교환
		System.out.println("a="+a);
		System.out.println("b="+b);
		tmp = a;
		a = b;
		b = tmp;
		
		int x = 0b01101;
		int y = 0b01011;
		
		
		

	}

}
